<!-- START #fh5co-hero -->

  <div class="container">
    <div class="row">
      <div class="col-md-12">
      <div class="fh5co-spacer fh5co-spacer-md"></div>

        <div class="col-md-6">
          <% case %>
            <% when params[:locale] == "zh_TW" || params[:locale] == "zh_CN" %>
              <img class="fh5co-align-left img-responsive" src="/assets/CH_INFORMED Logo.png" alt="informed logo" >
            <% when params[:locale] == "vi" %>
              <img class="fh5co-align-left img-responsive" src="/assets/VIET_INFORMED Logo.png" alt="informed logo" >
            <% when params[:locale] == "hmn" %>
              <img class="fh5co-align-left img-responsive" src="/assets/HMONG_INFORMED Logo.png" alt="informed logo" >
            <% when params[:locale] == "en" %>
              <img class="fh5co-align-left img-responsive" src="/assets/ENG_INFORMED Logo.png" alt="informed logo" >
            <% end %>
        </div>
        <div class="col-md-6 clearfix fh5co-header">
          <div class="fh5co-spacer fh5co-spacer-lg"></div>
          <h1 class="h1 fh5co-heading fh5co-no-margin-bottom"><%= t('about.title') %></h1>
        </div>
        <div class="clearfix fh5co-header">
          <div class="fh5co-spacer fh5co-spacer-sm"></div>
          <h4 class="h5 fh5co-heading-sub"><%= t('about.mission1') %></h4>
          <div class="fh5co-spacer fh5co-spacer-sm"></div>
          <h4 class="h5 fh5co-heading-sub"><%= t('about.mission2') %></h4>
          <div class="fh5co-spacer fh5co-spacer-sm"></div>
        </div>
      </div>
    </div>
  </div>



<% if @callouts.any? %>
  <%= render 'callouts', collection: @callouts %>
  <div class="fh5co-spacer fh5co-spacer-xlg"></div>
<% end %>

<%= render 'researchers', collection: @researchers %>

<%= render 'cabmembers', collection: @cab_members %>

