<p id="notice"><%= notice %></p>

<h1>Statistics</h1>

<table>
  <thead>
    <tr>
      <th>En title</th>
      <th>En description</th>
      <th>En link name</th>
      <th>Zh tw title</th>
      <th>Zh tw description</th>
      <th>Zh tw link name</th>
      <th>Zh cn title</th>
      <th>Zh cn description</th>
      <th>Zh cn link name</th>
      <th>Vi title</th>
      <th>Vi description</th>
      <th>Vi link name</th>
      <th>Hmn title</th>
      <th>Hmn description</th>
      <th>Hmn link name</th>
      <th>En Link url</th>
      <th>Zh TW Link url</th>
      <th>Zh CN Link url</th>
      <th>Vi Link url</th>
      <th>Hmn Link url</th>
      <th>Category</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @statistics.each do |statistic| %>
      <tr>
        <td><%= statistic.en_title %></td>
        <td><%= statistic.en_description %></td>
        <td><%= statistic.en_link_name %></td>
        <td><%= statistic.zh_tw_title %></td>
        <td><%= statistic.zh_tw_description %></td>
        <td><%= statistic.zh_tw_link_name %></td>
        <td><%= statistic.zh_cn_title %></td>
        <td><%= statistic.zh_cn_description %></td>
        <td><%= statistic.zh_cn_link_name %></td>
        <td><%= statistic.vi_title %></td>
        <td><%= statistic.vi_description %></td>
        <td><%= statistic.vi_link_name %></td>
        <td><%= statistic.hmn_title %></td>
        <td><%= statistic.hmn_description %></td>
        <td><%= statistic.hmn_link_name %></td>
        <td><%= statistic.en_link_url %></td>
        <td><%= statistic.zh_tw_link_url %></td>
        <td><%= statistic.zh_cn_link_url %></td>
        <td><%= statistic.vi_link_url %></td>
        <td><%= statistic.hmn_link_url %></td>
        <td><%= statistic.category %></td>
        <td><%= link_to 'Show', statistic %></td>
        <td><%= link_to 'Edit', edit_statistic_path(statistic) %></td>
        <td><%= link_to 'Destroy', statistic, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Statistic', new_statistic_path %>
